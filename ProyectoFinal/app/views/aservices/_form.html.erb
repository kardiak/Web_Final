<% nombre||="Create" %>	
<h1><%= nombre %> Aservice</h1>
<%= form_for(@aservice) do |f| %>
	<% @aservice.errors.full_messages.each do |message| %>
		<div class="be-red white top-space">
			*<%= message %>
		</div>
	<% end %>
	<div >
		<%= f.text_field :name, placeholder:"Servicio", class:"form-control"%>
	</div>
	<div>
		<%= f.text_field :ip,placeholder:"Ip", class:"form-control" %>
	</div>
	<div>
		<%= f.text_field :protocol, placeholder:"Protocollo", class:"form-control" %>
	</div>
	<div>
		<%= f.submit "Guardar",class:"btn be-red white" %> <%= link_to "Cancelar", @aservice %>
	</div>
<%end%>
